#include <stdio.h>

int main() {
    char name[10];
    int age;
    float cgpa;
    char grade;
    scanf("%9s", name);
    scanf("%d", &age);
    scanf("%f", &cgpa);
    while (getchar() != '\n');
    scanf("%c", &grade);
    int temp = (int)(cgpa * 100);
    float tcgpa = temp / 100.0;
    printf("Name: %s\n", name);
    printf("Age: %d\n", age);
    printf("CGPA: %.2f\n", tcgpa);
    printf("Grade: %c\n", grade);

    return 0;
}
